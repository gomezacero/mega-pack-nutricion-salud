{
  "name": "educadox-landings",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "workspaces": [
    "landings/*"
  ],
  "scripts": {
    "dev": "npm run dev --workspace=superpack-saludable",
    "dev:home": "npm run dev --workspace=home",
    "build": "npm run build --workspace=superpack-saludable",
    "build:home": "npm run build --workspace=home",
    "build:all": "npm run build --workspace=home && npm run build --workspace=superpack-saludable",
    "preview": "npm run preview --workspace=superpack-saludable"
  },
  "dependencies": {
    "@google/genai": "^1.35.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^5.0.0",
    "typescript": "~5.8.2",
    "vite": "^6.2.0"
  }
}
