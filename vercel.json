{
  "buildCommand": "npm run build:all",
  "outputDirectory": "dist",
  "rewrites": [
    { "source": "/superpack-saludable", "destination": "/superpack-saludable/index.html" },
    { "source": "/superpack-saludable/", "destination": "/superpack-saludable/index.html" },
    { "source": "/superpack-saludable/:path*", "destination": "/superpack-saludable/:path*" },
    { "source": "/((?!superpack-saludable|assets).*)", "destination": "/index.html" }
  ]
}
